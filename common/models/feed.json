{
  "name": "Feed",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "comments": "the name of the feed"
    },
    "featured": {
      "type": "boolean",
      "required": false,
      "comments": "a featured feed is better promoted in search and suggestions"
    }
  },
  "validations": [],
  "relations": {
    "seeder": {
      "type": "belongsTo",
      "model": "Seeder",
      "foreignKey": ""
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "foreignKey": ""
    },
    "tags": {
      "type": "hasAndBelongsToMany",
      "model": "Tag",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
