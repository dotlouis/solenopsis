{
  "name": "Seeder",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "devices": {
      "type": [
        "object"
      ],
      "comments": "the devices registered for this account. Used for push notifications"
    },
    "featured": {
      "type": "boolean",
      "comments": "a featured seeder is better promoted in search and suggestions"
    },
    "firstName": {
      "type": "string",
      "comments": "the first name of the seeder"
    },
    "lastName": {
      "type": "string",
      "comments": "the last name of the seeder"
    },
    "name": {
      "type": "string",
      "comments": "the name (firstname + lastname) of the seeder"
    },
    "settings": {
      "type": "object",
      "comments": "the seeder' settings and preferences"
    },
    "type": {
      "type": "string",
      "comments": "a string specifying the nature of the seeder (USER, UNIVERSITY...)"
    }
  },
  "validations": [],
  "relations": {
    "feeds": {
      "type": "hasMany",
      "model": "Feed",
      "foreignKey": ""
    },
    "tags": {
      "type": "hasMany",
      "model": "Tag",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "facebook"
    }
  ],
  "methods": []
}
